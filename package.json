{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port 5000 --hostname 0.0.0.0",
    "build": "prisma generate && next build",
    "start": "next start --port 3000 --hostname 0.0.0.0",
    "lint": "next lint",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "seed": "ts-node -O '{\"module\":\"CommonJS\"}' prisma/seed.ts",
    "seed:audit": "ts-node -O '{\"module\":\"CommonJS\"}' prisma/seedAuditChecks.ts",
    "seed:comprehensive": "ts-node -O '{\"module\":\"CommonJS\"}' prisma/seedComprehensive.ts",
    "fmcsa:import": "ts-node -O '{\"module\":\"CommonJS\"}' scripts/fmcsa-import.ts",
    "fmcsa:autosync": "ts-node -r tsconfig-paths/register -O '{\"module\":\"CommonJS\"}' scripts/fmcsa-autosync.ts",
    "test": "jest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "seed:e2e": "ts-node -r tsconfig-paths/register -O '{\"module\":\"CommonJS\"}' scripts/e2e/seed.ts",
    "run:e2e": "ts-node -r tsconfig-paths/register -O '{\"module\":\"CommonJS\"}' scripts/e2e/run.ts"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@playwright/test": "^1.57.0",
    "@prisma/client": "^6.18.0",
    "@ringcentral/sdk": "^5.0.6",
    "@sendgrid/mail": "^8.1.6",
    "@supabase/supabase-js": "^2.86.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/busboy": "^1.5.4",
    "@types/formidable": "^3.4.6",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^3.0.3",
    "busboy": "^1.6.0",
    "cookie": "^1.1.1",
    "csv-parse": "^6.1.0",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "formidable": "^3.5.4",
    "google-auth-library": "^10.5.0",
    "googleapis": "^168.0.0",
    "next": "^15.5.9",
    "next-auth": "^4.24.13",
    "openai": "^6.9.1",
    "playwright": "^1.57.0",
    "prisma": "^6.18.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "2.4.1",
    "recharts": "^3.5.1",
    "swr": "^2.3.7",
    "tailwindcss": "^4.0.15",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "twilio": "^5.10.7",
    "uuid": "^13.0.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@types/cookie": "^0.6.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.13.11",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.23.0",
    "eslint-config-next": "^15.2.3",
    "jest": "^30.2.0",
    "node-mocks-http": "^1.17.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
